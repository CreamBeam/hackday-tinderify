{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "export NODE_ENV=DEV && vite --host",
    "start:test": "export NODE_ENV=TEST && vite --host --open false",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "test": "vitest run",
    "lint": "eslint .",
    "emulators:all": "npm run build && firebase emulators:start",
    "emulators:functions": "firebase emulators:start --only functions",
    "deploy": "npm run build && firebase deploy --only hosting"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "axios": "^0.27.2",
    "esbuild": "0.16.9",
    "firebase": "^9.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "stripe": "^10.13.0",
    "typescript": "^4.6.2",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/babel-core": "^6.25.7",
    "@types/node": "^18.8.4",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-router": "^5.1.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/spotify-web-playback-sdk": "^0.1.15",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.22.0",
    "eslint-config-react-app": "^7.0.1",
    "jsdom": "^20.0.3",
    "sass": "^1.56.0",
    "vite": "^4.0.2",
    "vite-plugin-svgr": "^2.3.0",
    "vite-tsconfig-paths": "^4.0.2",
    "vitest": "^0.25.7"
